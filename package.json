{
  "name": "ai-workforce-fhinck",
  "version": "1.0.0",
  "description": "Real-time dashboard for AI agents visualization",
  "main": "src/js/app.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "publish": "vite build && firebase deploy --only hosting",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:all": "vite build && firebase deploy",
    "test:sim": "node tests/simulate-agents.js",
    "test:sim:full": "node tests/simulate-agents.js --scenario=full",
    "test:sim:loop": "node tests/simulate-agents.js --loop",
    "test:sim:fast": "node tests/simulate-agents.js --fast",
    "test:interactive": "node tests/interactive.js"
  },
  "dependencies": {
    "firebase": "^10.7.1"
  },
  "devDependencies": {
    "firebase-admin": "^13.6.0",
    "vite": "^7.2.7"
  },
  "keywords": [
    "ai",
    "dashboard",
    "firebase",
    "realtime"
  ],
  "author": "",
  "license": "MIT"
}
